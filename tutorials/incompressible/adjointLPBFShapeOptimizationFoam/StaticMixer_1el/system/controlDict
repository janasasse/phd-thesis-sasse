FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          controlDict;
}

application     adjointLPBFShapeOptimizationFoam;

startFrom       latestTime;

startTime       1981;

stopAt          endTime;

endTime         2481;

deltaT          1;

writeControl    adjustableRunTime;

writeInterval   100;

purgeWrite      5;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


