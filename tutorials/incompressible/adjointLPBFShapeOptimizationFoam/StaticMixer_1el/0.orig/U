FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type                flowRateInletVelocity;
        volumetricFlowRate  7.54830918e-6; // 20 kg/h, 736 kg/m^3
    }

    outlet
    {
        type            zeroGradient;
    }

    defaultName
    {
        type            noSlip;
    }
}

// ************************************************************************* //